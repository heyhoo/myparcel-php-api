{
    "name": "mvdnbrk/myparcel-php-api",
    "description": "MyParcel API client for PHP",
    "keywords": [
        "postnl",
        "myparcel"
    ],
    "homepage": "https://github.com/mvdnbrk/myparcel-php-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "homepage": "https://github.com/mvdnbrk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "composer/ca-bundle": "^1.2",
        "guzzlehttp/guzzle": "^6.4 || ^7.0",
        "tightenco/collect": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^4.2",
        "phpunit/phpunit": "^8.5 || ^9.0",
        "symfony/var-dumper": "^4.4 || ^5.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\MyParcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\MyParcel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
